#include <iostream>

using namespace std;

int dot_product (int lista1[],int lista2[]){
  float suma=0;
  for(int i=1; i<=3; i++){
    int lista1[i];
    cout<<"dime el numero de lista1, posicion "<<i<<": "<<endl;
    cin>>lista1[i];
  }
  for (int i=1;i<=3;i++){
    int lista2[i];
    cout<<"dime el numero de lista2, posicion "<<i<<": "<<endl;
    cin>> lista2[i];
  }
  for (int i=1;i<=3;i++){
    suma= suma + lista1[i] * lista2[i];
  }
  return suma;
}

int main(){
  int lista1[3],lista2[3];
  cout<<dot_product(lista1,lista2)<<endl;
  return 0;
}
